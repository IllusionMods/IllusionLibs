<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>IllusionLibs.BepInEx</id>
    <version>5.4.12</version>
    <description>Unity game patcher and plugin framework</description>
    <authors>Bepis, GHorsington</authors>
    <projectUrl>https://github.com/BepInEx/BepInEx</projectUrl>
    <dependencies>
      <!-- Remember to change the harmony version to the exact version that ships with that bepin version -->
      <dependency id="IllusionLibs.BepInEx.Harmony" version="[2.5.1]" />
    </dependencies>
  </metadata>
  <files>
    <file src="libraries\BepInEx.dll" target="lib\net35" />
    <file src="libraries\BepInEx.xml" target="lib\net35" />
    <file src="..\install.ps1" target="tools" />
    <file src="$id$.targets" target="build" />
  </files>
</package>